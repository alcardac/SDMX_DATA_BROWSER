#main-treeview {
  overflow: hidden !important;
}

li[aria-expanded="true"]>.jstree-anchor>.icon-folder:before {
    content: '\e8ee' !important;
}
/*
.jstree-default .jstree-open > i.jstree-ocl:before
{
    content: '\e916' !important;
}
.jstree-default .jstree-close > i.jstree-ocl:before
{
    content: '\e919' !important;
}
*/
.jstree-anchor:hover {
    border:0px !important;
}

.tree-button {
    font-weight: bold;
    margin: 5px -8px 5px 9px !important;
    padding: 0px 10px 0px 0px !important;
    text-align: left !important;
    text-decoration: none;
    width: 156px !important;
    font-size: 11px;
}

.tree-button i {
    margin:0px -5px 0px -2px;
}

.tree-button:hover {
    border-bottom:0px !important;
    cursor: pointer;
}

#main-treeview a {
    height: auto;
    width:90%;
    white-space:pre-line !important;
}

#main-treeview a>i {
    margin:0px -2px 0px -8px;
}

ul.jstree-container-ul {
    background: none !important; 
}

.jstree-default .jstree-node,
.jstree-default .jstree-icon {
    /*background-image:none !important;*/
}

.jstree-hovered,
.jstree-clicked {
    
    background:none !important;
    border-radius: 0px !important;
    border:0px !important;
    box-shadow: none !important;
 }

li[rel="category"] {
    margin:1px 0px;
}

li[rel="category"] {
    background-color: #D1D1D1 !important;
}

li[rel="category-scheme"]>a {
    padding: 0px 0px 0px 4px !important;
}

li[rel="category"]>a {
    height: 100%;    
    line-height: 2;
    padding: 0px 0px 0px 4px !important;
    width:100%;
}

li[rel="dataflow"]>a,
li[rel="virtual-dataflow"]>a {
    height: 100%;    
    line-height: 2;
    padding:0px 0px 0px 2px !important;
    width:100%;
}

li[rel="dataflow"]>a:hover ,
li[rel="virtual-dataflow"]>a:hover{
    padding: 0px 0px 0px 2px !important;
}

li[rel="dataflow"]>a.jstree-clicked ,
li[rel="virtual-dataflow"]>a.jstree-clicked{
    /*background: #DA0D14 !important;
    color: #FFF !important;*/
    padding: 0px 0px 0px 4px !important;
}

li[rel="virtual-dataflow"]>a>i {
    color:#ffd800;
} 

.tree-button i {
    color:#DA0D14;
}

/*TREE*/

li[rel="category"][aria-level^="2"] {
    background-color: #FFF !important;
}

li.jstree-open[rel="category"] {
    background-color: #A2A9AE !important;
    padding-bottom: 1px;
}

li.jstree-open[rel="category"] > a {
    color: #FFF !important;
}

li[rel="category"]{
    background-color: #D1D1D1 !important;
}

li[rel="category"]>a {
    height: 100%;    
    line-height: 2;
    padding:0px 0px 0px 2px !important;
    width:100%;
}

li:not(.jstree-open)[rel="category"][aria-level^="2"]>a:hover {
    background: #DA0D14 !important;
    color: #FFF !important;
    padding: 0px 0px 0px 2px !important;
}

li[rel="dataflow"],
li[rel="virtual-dataflow"] {
    background-color: #EEE !important;
}

li[rel="dataflow"]>a {
    height: 100%;    
    line-height: 2;
    padding:0px 0px 0px 2px !important;
    width:100%;
}

li[rel="dataflow"]>a:not(.jstree-clicked):hover {
    background: #DA0D14 !important;
    color: #FFF !important;
    padding: 0px 0px 0px 2px !important;
}

li[rel="dataflow"]>a.jstree-clicked {
    color: #DA0D14 !important;
    padding: 0px 0px 0px 2px !important;
}


/*TREE CUSTOM*/

.jstree li[rel="category-scheme"] > a > .jstree-icon:before,
.jstree li[rel="category"] > a > .jstree-icon:before,
.jstree li[rel="dataflow"] > a > .jstree-icon:before{
  color: #DA0D14;
}

.jstree li:not(.jstree-open)[rel="category"][aria-level^="2"] > a:hover > .jstree-icon:before{
  color: #FFF;
}

.jstree li[rel="dataflow"] > a:not(.jstree-clicked):hover > .jstree-icon:before{
  color: #FFF;
}

li[rel="dataflow"] > a.jstree-clicked > .jstree-icon:before{
  color: #DA0D14;
}

.removeRow i {
    color:#fff;
}

/*CHECKBOXES STYLE*/

.jstree-checkbox {
    background:none !important;
    font-family: "all-icon-font-16x16";
    font-size: 14px;
    font-style: normal;
    font-weight: normal;
    speak: none;
 
    display: inline-block;
    text-decoration: inherit;
    width: 1em;
    margin-right: .2em;
    text-align: center;
    /* opacity: .8; */
 
    /* For safety - reset parent styles, that can break glyph codes*/
    font-variant: normal;
    text-transform: none;
     
    /* fix buttons height, for twitter bootstrap */
    line-height: 1em;
 
    /* Animation center compensation - margins should be symmetric */
    /* remove if not needed */
    margin-left: .2em;
 
    /* you can be more comfortable with increased icons size */
    /* font-size: 120%; */
 
    /* Uncomment for 3D effect */
    /* text-shadow: 1px 1px 1px rgba(127, 127, 127, 0.3); */
}
.jstree-anchor > i.jstree-icon.jstree-checkbox:before  { content: '\e98f'; }
.jstree-anchor > i.jstree-icon.jstree-checkbox:hover:before { color:#DA0D14 ;content: '\e98f'; }
.jstree-anchor.jstree-checked > i.jstree-icon.jstree-checkbox:before { content: '\e98e';padding: 0px 0px 0px 2px;}
.jstree-anchor.jstree-checked > i.jstree-icon.jstree-checkbox:hover:before { color:#DA0D14 ;content: '\e98e'; }