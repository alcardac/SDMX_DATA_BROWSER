html {
    height:100%;
    overflow:auto;
}
body {
    color: #000;
    font-family: Arial, Tahoma, Verdana;
    font-size: 12px;
    height:100%;
    margin: 0px !important;
}

/* unvisited link */
body a:link {                                                   
    color: #000;
    text-decoration:none;
}

/* visited link */
body a:visited {
    color: #000;
    text-decoration:none;
}

/* mouse over link */
body a:hover {
    border-bottom: 1px solid;
    color: #000;
    text-decoration:none;
}

/* selected link */
body a:active {
    color: #000;
    text-decoration:none;
}

.clear-box {
    clear:both;
}

.Maincontainer{
    height:100%;
    width: 100%;
    margin: 0 auto;
    /*max-width: 100%;
    min-width: 950px; */
}

header {
    background:#FFF !important;
    color:#666;
    height: 70px;
    padding:0px 0px 0px 0px;
    /*position:fixed;
    top:0;*/
    width: 100%;
}

/* header link */
header a:link {
    border:0px;
    color: #666;
    text-decoration:none;
}
header a:visited {
    color: #666;
    text-decoration:none;
}
header a:hover, a:focus {
    border-bottom: 2px solid;
    color: #666;
    text-decoration:none;
}
header a:active {
    color: #666;
    text-decoration:none;
}

header #header-left a:hover, a:focus{
    border-bottom: none !important;
}

header #header-left {
    float:left;
    font-size: 25px;
