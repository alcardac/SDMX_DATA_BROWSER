.color-option {
    cursor:pointer;
    display: inline-block;
    width: 30%;
    padding: 5px 15px 15px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    margin-bottom: 3px;
}

.color-option.selected, .color-option:hover {
    background: #EAEAEA;
}

.color-palette {
    width: 100%;
    border-spacing: 0;
    border-collapse: collapse;
}

.color-palette td {
    height: 20px;
    padding: 0;
    border: none;
}

.color-option label {
    cursor:pointer;
    margin: .25em 0 .5em!important;
    display: inline-block;
    line-height: 1.4em;
}

input[type=radio] {
    border: 1px solid #bbb;
    border-radius:50%;
    background: #fff;
    color: #555;
    clear: none;
    cursor: pointer;
    display: inline-block;
    font-family: "all-icon-font-16x16";
    line-height: 0;
    height: 16px;
    margin: -4px 4px 0 0;
    outline: 0;
    padding: 0!important;
    text-align: center;
    vertical-align: middle;
    width: 16px;
    min-width: 16px;
    -webkit-appearance: none;
}

input[type=radio]:checked:before {
    content: '\e990';     
    text-indent: -9999px;
    -webkit-border-radius: 50px;
    border-radius: 50px;
    font-size: 9px;
    width: 6px;
    height: 6px;
    margin: 3px;
    line-height: 15px;
    color: #666;
}

